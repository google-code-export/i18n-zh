msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7945 ../../en/build/defguide5.xml:8458
msgid "Suppressed. This element provides data for processing but it is not expected to be rendered directly."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7949
msgid "The processing expectations of <tag>callout</tag>s are likely to deserve special consideration for interchange. See <xref linkend=\"app-interchange\"/>."
msgstr ""

#.(tag), (quote)
#: ../../en/build/defguide5.xml:7957 ../../en/build/defguide5.xml:8042 ../../en/build/defguide5.xml:8274 ../../en/build/defguide5.xml:8496
msgid "calspair"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7957
msgid "The coordinates are expressed using the semantics of the <acronym>CALS</acronym> graphic attributes. The format of the coordinates is “x1,y1 x2,y2”. This identifies a rectangle with the lower-left corner at (x1,y1) and the upper-right corner at (x2,y2). The X and Y coordinates are integers in the range 0 to 10000; they express a percentage of the total distance from 0.00 to 100.00%. The lower-left corner is (0,0)."
msgstr ""

#.(tag), (quote)
#: ../../en/build/defguide5.xml:7963 ../../en/build/defguide5.xml:8050 ../../en/build/defguide5.xml:8282 ../../en/build/defguide5.xml:8504
msgid "linecolumn"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7963
msgid "The coordinates are expressed using lines and columns. The format of the coordinates is “line column.” In a graphic context, the meaning of this unit is unspecified."
msgstr ""

#.(tag), (quote)
#: ../../en/build/defguide5.xml:7966 ../../en/build/defguide5.xml:8066 ../../en/build/defguide5.xml:8298 ../../en/build/defguide5.xml:8520
msgid "linerange"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7966
msgid "The coordinates are expressed using lines. The format of the coordinates is “startingline endingline.” In a graphic context, the meaning of this unit is unspecified."
msgstr ""

#.(tag), (quote)
#: ../../en/build/defguide5.xml:7969 ../../en/build/defguide5.xml:8058 ../../en/build/defguide5.xml:8290 ../../en/build/defguide5.xml:8512
msgid "linecolumnpair"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7969
msgid "The coordinates are expressed as a continuous flow of characters. The format of the coordinates is “line1 col1 line2 col2”. This identifies a flow of characters that begins at col1 of line1 and extends to col2 of line2. If line1 and line2 are different, then the region includes all of the intervening lines (including text that occurs before col1 and after col2). In other words, this unit does not specify a rectangle. In a graphic context, the meaning of this unit is unspecified."
msgstr ""

#.(tag), (term)
#: ../../en/build/defguide5.xml:7977 ../../en/build/defguide5.xml:8014 ../../en/build/defguide5.xml:8246 ../../en/build/defguide5.xml:8468
msgid "otherunits"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7977
msgid "If specified, then the <tag class=\"attribute\">otherunits</tag> attribute is expected to identify the units in some implementation-specific way."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7952
msgid "The <tag class=\"attribute\">coords</tag>, which are required, identify the location of the region. The coordinates are <acronym>CDATA</acronym>; how they are interpreted depends on the <tag class=\"attribute\">units</tag> specified: <placeholder-1/> The <tag class=\"attribute\">units</tag> attribute is not required, if it is not specified, the semantics of the coordinates must be inherited from the surrounding <tag>areaspec</tag> or <tag>areaset</tag> element or implied in some implementation-specific manner."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7987
msgid "In processing systems in which the mark is inserted automatically, the <tag class=\"attribute\">label</tag> attribute is provided as a mechanism for specifying what the mark should be."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:7991
msgid "The author may point to any relevant information with <tag class=\"attribute\">linkends</tag>. DocBook does not specify a semantic for these links. One possible use would be for providing a link back to the appropriate <tag>callout</tag> in an online environment."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8003
msgid "Provides the coordinates of the area. The coordinates must be interpreted using the <tag class=\"attribute\">units</tag> specified."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8010
msgid "Specifies an identifying number or string that may be used in presentation. The area label might be drawn on top of the figure, for example, at the position indicated by the coords attribute."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8016 ../../en/build/defguide5.xml:8248 ../../en/build/defguide5.xml:8470
msgid "Identifies the units used in the <tag class=\"attribute\">coords</tag> attribute when the <tag class=\"attribute\">units</tag> attribute is <quote>other</quote> . This attribute is forbidden otherwise."
msgstr ""

#.(term)
#: ../../en/build/defguide5.xml:8023 ../../en/build/defguide5.xml:8078 ../../en/build/defguide5.xml:8255 ../../en/build/defguide5.xml:8310 ../../en/build/defguide5.xml:8477 ../../en/build/defguide5.xml:8532 ../../en/build/defguide5.xml:14367
msgid "units"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8025 ../../en/build/defguide5.xml:8257 ../../en/build/defguide5.xml:8479
msgid "Identifies the units used in the coords attribute The default units vary according to the type of callout specified: <tag class=\"attvalue\">calspair</tag> for graphics and <tag class=\"attvalue\">linecolumn</tag> for line-oriented elements."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8045 ../../en/build/defguide5.xml:8277 ../../en/build/defguide5.xml:8499
msgid "Coordinates expressed as a pair of CALS graphic coordinates."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:8053 ../../en/build/defguide5.xml:8285 ../../en/build/defguide5.xml:8507
