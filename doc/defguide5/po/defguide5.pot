msgid "A collection of reference entries"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "reference ::="
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>reference</tag> is a collection of <tag>refentry</tag>s. In a <tag>book</tag>, a <tag>reference</tag> can occur at either the <tag>part</tag> or <tag>chapter</tag> level."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Reference pages are usually bound together by topic; in traditional <acronym>UNIX</acronym> documentation they are most frequently bound into volumes. See <tag>manvolnum</tag>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Formatted as a displayed block. A <tag>reference</tag> often introduce a forced page break and may start on the next recto page. Frequently, they also produce a separator page, on which may be printed the content of the <tag>partintro</tag>."
msgstr ""

