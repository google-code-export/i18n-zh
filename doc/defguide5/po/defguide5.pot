#.(refpurpose)
#: xml/defguide5.xml:65535
msgid "A wrapper for the main content of a table, or part of a table"
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "tgroup ::="
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "<tag>thead</tag>? <phrase role=\"pattern\">(db.cals.thead)</phrase>"
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "<tag>tfoot</tag>? <phrase role=\"pattern\">(db.cals.tfoot)</phrase>"
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "<tag>tbody</tag> <phrase role=\"pattern\">(db.cals.tbody)</phrase>"
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "<emphasis role=\"bold\">cols</emphasis> (positiveInteger)"
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "A <tag>tgroup</tag> surrounds a logically complete portion of a table. Most tables consist of a single <tag>tgroup</tag>, but complex tables with widely varying column specifications may be easier to code using multiple <tag>tgroup</tag>s."
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "The <tag>tgroup</tag> specifies the number of columns in the table, and contains all of the header, body, and footer rows, along with any additional column or span specifications necessary to express the geometry of the table."
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "Most of the properties of rows, columns, and cells inherit their default characteristics from the enclosing <tag>tgroup</tag>."
msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "The number of columns in the table. Must be an integer greater than zero."
msgstr ""

