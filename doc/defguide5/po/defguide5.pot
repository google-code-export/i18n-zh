msgstr ""

#.(para)
#: xml/defguide5.xml:65535
msgid "This software is provided <quote>as is</quote>, without expressed or implied warranty."
msgstr ""

#.(refmiscinfo)
#: xml/defguide5.xml:65535
msgid "db.refclass"
msgstr ""

#.(refpurpose)
#: xml/defguide5.xml:65535
msgid "The scope or other indication of applicability of a reference entry"
msgstr ""

