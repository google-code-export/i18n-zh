#.(title)
#: xml/defguide5.xml:336
msgid "Conventions Used in This Book"
msgstr ""

#.(para)
#: xml/defguide5.xml:339
msgid "A <tag role=\"nonexistant\">monospaced font</tag> is used for element and attribute names."
msgstr ""

#.(para)
#: xml/defguide5.xml:344
msgid "A <literal>monospaced font</literal> is also used for program examples, attribute value literals, start- and end-tags, and source code example text."
msgstr ""

#.(para)
#: xml/defguide5.xml:350
msgid "An <replaceable>italic monospaced font</replaceable> is used for <quote>replaceable</quote> text or variables. Replaceable text is text that describes something you're supposed to type, like a <replaceable>filename</replaceable>, in which the word <quote>filename</quote> is a placeholder for the actual filename."
msgstr ""

#.(para)
#: xml/defguide5.xml:358
msgid "<filename>Italic</filename> is used for filenames and (in the print version of the book) <acronym>URL</acronym>s."
msgstr ""

#.(para)
#: xml/defguide5.xml:363
msgid "<link href=\"http://docbook.org/\"><acronym>URL</acronym>s</link> are presented in parentheses after the name of the resource they describe in the print version of the book."
msgstr ""

