msgid "Installation"
msgstr "安装"

#: build/en/defguide5.xml:4668(title)
msgid "Installing DocBook"
msgstr "安装 DocBook"

#: build/en/defguide5.xml:4671(title)
msgid "Installing the DocBook RELAX NG Grammar"
msgstr "安装 DocBook RELAX NG 文法"

#: build/en/defguide5.xml:4673(para) build/en/defguide5.xml:4680(para)
#: build/en/defguide5.xml:4687(para) build/en/defguide5.xml:4694(para)
